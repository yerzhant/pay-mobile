  <SQLQuery>
    <![CDATA[SELECT Contracts.ID$,
       Contracts.DATE$,
       Contracts.USER$,
       Contracts.CLIENT_ID$,
       Contracts.STATUS,
       Contracts.OPEN_DATE,
       Contracts.APPLIED_AT,
       Contracts.PHONE_NUMBER,
       Contracts.PASSWORD_TRIES,
       Contracts.CLOSE_DATE,
       Contracts.A_NUMBER,
       Contracts.CID$
  FROM PAY_MOBILE.CONTRACTS Contracts
 WHERE Contracts.STATUS$ = :RS AND Contracts.CLIENT_ID$ IN (
SELECT C.ID$
  FROM PAY_MOBILE.USERS_BRANCHES UB, PAY_MOBILE.USERS U, PAY_MOBILE.CLIENTS C
 WHERE UB.USER_ID$ = U.ID$ AND (UB.BRANCH_ID$ = C.BRANCH_ID$ OR C.BRANCH_ID$ IS NULL)
   AND UB.STATUS$ = 'C' AND U.STATUS$ = 'C' AND C.STATUS$ = 'C'
   AND U.SYS_NAME = :currentUser
)]]>
  </SQLQuery>
