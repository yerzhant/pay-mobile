<?xml version='1.0' encoding='UTF-8'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.1" xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces/rich">
  <jsp:directive.page contentType="text/html;charset=UTF-8"/>
  <af:pageTemplateDef var="attrs">
    <af:panelStretchLayout id="pt_psl1" topHeight="20px">
      <f:facet name="center">
        <af:panelSplitter id="pt_ps1" orientation="vertical">
          <f:facet name="first">
            <af:facetRef facetName="top"/>
          </f:facet>
          <f:facet name="second">
            <af:facetRef facetName="bottom"/>
          </f:facet>
        </af:panelSplitter>
      </f:facet>
      <f:facet name="top">
        <af:outputText value="#{attrs.title}" id="pt_ot1" inlineStyle="font-weight:bold;"/>
      </f:facet>
    </af:panelStretchLayout>
    <af:xmlContent>
      <component xmlns="http://xmlns.oracle.com/adf/faces/rich/component">
        <display-name>History</display-name>
        <facet>
          <facet-name>top</facet-name>
        </facet>
        <facet>
          <facet-name>bottom</facet-name>
        </facet>
        <attribute>
          <attribute-name>title</attribute-name>
          <attribute-class>java.lang.String</attribute-class>
        </attribute>
      </component>
    </af:xmlContent>
  </af:pageTemplateDef>
</jsp:root>
