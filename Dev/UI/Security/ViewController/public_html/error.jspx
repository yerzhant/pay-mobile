        <af:pageTemplate viewId="/WEB-INF/main-template.jspx" id="pt1">
          <f:facet name="contents">
            <af:panelGroupLayout id="pgl1" halign="center" layout="vertical">
              <af:decorativeBox id="db1" topHeight="0" theme="light">
