                  <af:panelStretchLayout id="psl1" bottomHeight="25px">
                    <f:facet name="bottom">
                      <af:panelGroupLayout id="pgl3" layout="vertical" halign="center">
                        <af:commandButton text="OK" id="cb1" action="toSetup"/>
                      </af:panelGroupLayout>
                    </f:facet>
                    <f:facet name="center">
                      <af:outputText value="#{controllerContext.currentViewPort.exceptionData.localizedMessage}" id="ot1"/>
                    </f:facet>
